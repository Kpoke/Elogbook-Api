<% include ../partials/header %>


<h1>Register a supervisor</h1>
<div class="container">
		<div class="myForm">
			<form action="/supervisor/register" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="username" placeholder="Supervisor Name">	
				</div>
				<div class="form-group">
					<input class="form-control" type="password" name="password" placeholder="Supervisor password">	
				</div>
				<button class="btn btn-primary ">Send Signup request</button>
			</form>
		</div>
</div>
	

<% include ../partials/footer %>